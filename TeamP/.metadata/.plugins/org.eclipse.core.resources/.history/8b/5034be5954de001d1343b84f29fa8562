package user;

import DBpackage.DBConnector;

public class UsersDAO extends DBConnector{
	public UsersDAO() {
		super();
	}
	
	
	//참고용 구문
	public int getTotalCount() {
	      int totalCount=0;
	      String sql="select count(*) from test.ingredient";
	      try {
	         psmt=con.prepareStatement(sql);
	         rs=psmt.executeQuery();
	         rs.next();
	         totalCount=rs.getInt(1);
	      } catch (Exception e) {
	         System.out.println("게시물 카운트 중 에러");
	         e.printStackTrace();
	      }
	      return totalCount;
	   }
	
	
	
}
